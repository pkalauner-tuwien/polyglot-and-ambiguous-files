extends index
block content
    form(action='/upload', method='post', enctype='multipart/form-data')
        label(for='inputFile') Choose file: 
        input#inputFile(type='file', name='file')
        br
        button(type='submit') Submit
    if msg
        | #{msg}
        br
    if imagepath
        a(href='view?image=#{imagepath}') Go to uploaded image
